msgstr "Vtičnik za financiranje ni na voljo"

msgid "plugins.generic.pflPlugin.settings.indexes"
msgstr "Seznam indeksiranja revije"

msgid "plugins.generic.pfl.displayName"
msgstr "Vtičnik za infromacije o objavi"

msgid "plugins.generic.pfl.metric"
msgstr "Metrika"

